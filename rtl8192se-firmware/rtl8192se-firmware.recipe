class RTL8192fw(PackageRecipe):

    name = 'rtl8192se-firmware'
    version = '2.6.0017.0507.2010'

    buildRequires = [ 'unzip:runtime' ]

    def setup(r):
        r.addArchive('rtl8192se_linux_%(version)s.tar.gz')
        r.Install('firmware/RTL8192SE/*', '/lib/firmware/RTL8192SE/')
        r.Move('/lib/firmware/RTL8192SE/Realtek-Firmware-License.txt', '/lib/firmware/LICENSE-%(name)s.txt')
        r.Flavor(exceptions='/lib/firmware/.*')

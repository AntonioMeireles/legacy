class RTL8192fw(PackageRecipe):

    name = 'rtl8192se-firmware'
    version = '2.6.0015.0127.2010'

    buildRequires = [ 'unzip:runtime' ]

    def setup(r):
        r.addArchive('rtl8192se_linux_2.6.0015.0127.2010.tar.gz')
        r.Move('firmware/RTL8192SE/*', '/lib/firmware/')
        r.Move('/lib/firmware/Realtek-Firmware-License.txt', '/lib/firmware/LICENSE-%(name)s.txt')
        r.Flavor(exceptions='/lib/firmware/.*')

class taglib(AutoPackageRecipe):
    name    = 'taglib'
    version = '1.4'

    buildRequires = ['zlib:devel', 'gcc-c++:runtime']

    def unpack(r):
        r.addArchive('http://developer.kde.org/~wheeler/files/src/%(name)s-%(version)s.tar.gz')

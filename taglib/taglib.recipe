class TagLib(CPackageRecipe):
   buildRequires = [ 'glibc:devel', 'libgcc:devellib', 'libstdc++:devel', 'zlib:devel' ]
	
   name = "taglib"
   version = "1.4"
   def setup(r):
	r.addArchive('http://developer.kde.org/~wheeler/files/src/%(name)s-%(version)s.tar.gz')
	r.Configure()
	r.Make()
	r.MakeInstall()

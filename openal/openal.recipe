class OpenAl(AutoPackageRecipe):

    name = "openal"
    version = "0.0.8"

    buildRequires = ['SDL:devel', 'esound:devel', 'pkgconfig:devel',
        'alsa-lib:devel', 'libvorbis:devel', 'smpeg:devel']

    def unpack(r):
        r.addArchive('http://www.openal.org/openal_webstf/downloads/')

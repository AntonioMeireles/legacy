class Libgcrypt(AutoPackageRecipe) :
    name = 'libgcrypt'
    version = '1.4.4'
    buildRequires = [ 'libgpg-error:devel', 'install-info:runtime' ]

    def unpack(r):
        r.addArchive('ftp://ftp.gnupg.org/gcrypt/%(name)s/')


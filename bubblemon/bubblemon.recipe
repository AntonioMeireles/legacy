loadRecipe('gnomepackage.recipe')
class bubblemon(GnomePackageRecipe):
    buildRequires = [ "glibc:devel" ]
    name = "bubblemon"
    version = "2.0.4"

    def setup(r):
        r.addArchive('http://savannah.nongnu.org/download/%(name)s/%(name)s-%(version)s.tar.gz')
        r.Configure()
        r.Make()
        r.MakeInstall()

loadSuperClass('gnomepackage')
class GnomeSpeech(GnomePackageRecipe):
    
    buildRequires = GnomePackageRecipe.buildRequires
    buildRequires.extend( [ 'libbonobo:devel', 'pkgconfig:devel',
        'ORBit2:devel', 'glib:devel' ] )

    name = 'gnome-speech'
    version = '0.4.0'

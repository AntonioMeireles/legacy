loadSuperClass('gnomepackage')
class GnomeSpeech(GnomePackageRecipe):
    
    buildRequires = [ 'libbonobo:devel', 'pkgconfig:devel',
        'ORBit2:devel', 'glib:devel', 'sun-jre:java' ] 

    name = 'gnome-speech'
    version = '0.4.3'


    def unpack(r):

	GnomePackageRecipe.unpack(r)
        r.Requires(exceptDeps='java: *')






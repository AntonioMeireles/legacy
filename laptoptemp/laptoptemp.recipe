class LaptopTemp(AutoPackageRecipe):

    buildRequires = [
        'gnome-python-desktop:python', 'gnome-python:python', 'pygtk:python'
        ]
    name = 'laptoptemp'
    version = '0.4'

    def unpack(r):
        #r.addArchive('http://infinito.f2o.org/laptoptemp/%(name)s-%(version)s.tar.gz')
        r.addArchive('%(name)s-%(version)s.tar.gz')

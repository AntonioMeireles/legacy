class qstat(PackageRecipe):
    buildRequires = [ "glibc:devel" ]
    name = "qstat"
    version = "2.8"

    def setup(r):
        r.addArchive('http://kent.dl.sourceforge.net/sourceforge/%(name)s/%(name)s-%(version)s.tar.gz')
        r.Configure()
        r.Make()
        r.MakeInstall()

#       Here are the docs

        r.Doc('CHANGES.txt', 'LICENSE.txt')

loadRecipe('gnomepackage.recipe')
class Libgnome(GnomePackageRecipe):
    
    buildRequires = GnomePackageRecipe.buildRequires
    buildRequires.extend([ 'zlib:devel', 'esound:devel', 'glib:devel',
                           'libbonobo:devel', 'GConf:devel',
                           'gnome-vfs:devel', 'libxml2:devel', 
                           'ORBit2:devel', 'libxslt:devel', 
                           'intltool:runtime' ])
    
    name = 'libgnome'
    version = '2.8.0'

    docs = GnomePackageRecipe.docs
    docs.remove('COPYING')

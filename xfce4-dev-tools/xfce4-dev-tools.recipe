class xfce4DevTools(AutoPackageRecipe):

    name = 'xfce4-dev-tools'
    version = '4.6.0'
    xfce = '4.6.0'

    buildRequires = ['gawk:runtime']

    def unpack(r):
        r.macros.xfce = r.xfce
        r.addArchive('http://mocha.xfce.org/archive/xfce-%(xfce)s/src/') 

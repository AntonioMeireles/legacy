class Openobex(AutoPackageRecipe):
    buildRequires = [ 'autoconf', 'docbook-utils', 'bluez-libs:devel', 'libusb:devel', 'pkgconfig:devel' ]

    name = "openobex"
    version = "1.3"

    def unpack(r):
        r.addArchive("mirror://sourceforge/openobex/%(name)s-%(version)s.tar.gz")

class Openobex(PackageRecipe):
    buildRequires = [ "autoconf", "docbook-utils", "bluez-libs:devel" ]

    name = "openobex"
    version = "1.0.1"

    def setup(r):
        r.addArchive("http://easynews.dl.sourceforge.net/sourceforge/openobex/%(name)s-%(version)s.tar.gz")
	r.Configure()
	r.Make()
        r.MakeInstall()


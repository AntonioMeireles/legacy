class Openobex(AutoPackageRecipe):
    buildRequires = [ 'autoconf', 'docbook-utils', 'bluez-libs:devel', 'libusb:devel' ]

    name = "openobex"
    version = "1.2"

    def unpack(r):
        r.addArchive("http://easynews.dl.sourceforge.net/sourceforge/openobex/%(name)s-%(version)s.tar.gz")


loadRecipe('gnomepackage.recipe')
class Gok(GnomePackageRecipe):
    
    buildRequires = GnomePackageRecipe.buildRequires
    buildRequires.extend([ 'at-spi:devel',  'gail:devel',
			   'intltool:runtime', 'libgnomeui:devel',
			   'libwnck:devel', 'gtk-doc:runtime',
			   'm4:runtime', 'gettext:runtime', 
                           'gnome-speech:devel' ])
    
    name = 'gok'
    version = '0.12.1'

    def setup(r):
        r.disableParallelMake()
        GnomePackageRecipe.setup(r)

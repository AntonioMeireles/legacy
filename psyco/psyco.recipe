class psyco(PackageRecipe):
    name = "psyco"
    version = "1.4"
    buildRequires = [ "python" ] 

    def setup(r):
        r.addArchive('http://puzzle.dl.sourceforge.net/sourceforge/%(name)s/%(name)s-%(version)s-src.tar.gz')

        r.Run('python setup.py build')
        r.Run('python setup.py install --root %(destdir)s')

class SOCat(AutoPackageRecipe):
    name = 'socat'
    version = '1.6.0.1'

    buildRequires = [
        'openssl:devel',
        'readline:devel',
        'tcp_wrappers:devel',
      ]

    def unpack(r):
        r.addArchive('http://www.dest-unreach.org/socat/download/')

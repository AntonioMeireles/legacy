loadSuperClass('xorgdatapackage')
class InputProto(XorgDataPackageRecipe):
    name = 'inputproto'
    version = '1.4.2.1'
    component = 'proto'

    def unpack(r):
        XorgDataPackageRecipe.unpack(r)
        r.addPatch('inputproto-1.4.diff', rpm='http://ftp.opensuse.org/pub/'
            'opensuse/distribution/SL-OSS-factory/inst-source/suse/src/'
            'xorg-x11-proto-devel-7.2-58.src.rpm')

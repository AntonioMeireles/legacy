loadRecipe('gnomepackage.recipe')
class Eog(GnomePackageRecipe):
    
    buildRequires = GnomePackageRecipe.buildRequires
    buildRequires.extend([ 'glib:devel', 'pango:devel', 'gtk:devel', 'libgnomeui:devel',
		'gail:devel', 'gnome-vfs:devel', 'gnome-keyring:devel', 'libgnomeprintui:devel',
		'libexif:devel', 'intltool:runtime', 'libjpeg:devel', 'scrollkeeper:runtime',
		'gettext:runtime', 'ORBit2:devel', 'GConf:devel', 'atk:devel', 'cairo:devel',
		'fontconfig:devel', 'freetype:devel', 'glibc:devel', 'lcms:devel',
		'libart_lgpl:devel', 'libbonobo:devel', 'libbonoboui:devel', 'libglade:devel', 
		'libgnome:devel', 'libgnomecanvas:devel', 'libgnomeprint:devel', 'libpng:devel',
		'libxml2:devel', 'popt:devel', 'gnome-desktop:devel', 'startup-notification:devel' ])

    name = 'eog'
    version = '2.13.5'
    preMakeInstall = 'GCONF_DISABLE_MAKEFILE_SCHEMA_INSTALL=1 '

    docs = ['AUTHORS', 'COPYING', 'ChangeLog', 'NEWS', 'README']


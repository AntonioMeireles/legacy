class VirtInst(PackageRecipe):
    buildRequires = [ 'libxml2:python', 'urlgrabber:python', 'python-setuptools:python' ]
    name = 'virtinst'
    version = '0.101.0'

    def setup(r):
        r.addArchive('http://virt-manager.et.redhat.com/download/sources/%(name)s/%(name)s-%(version)s.tar.gz')
	r.PythonSetup()

class KibaPlugins(AutoPackageRecipe):
    name = 'kiba-plugins'
    version = '0.2'
 
    buildRequires = ['kiba-dock']
 
    def unpack(r):
        r.addSvnSnapshot('https://kibadock.svn.sourceforge.net/svnroot/kibadock/trunk/kiba-plugins/')
    def configure(r):
        r.Run('./autogen.sh')
	r.Configure()


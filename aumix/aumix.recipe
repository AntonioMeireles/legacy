class Aumix(PackageRecipe):
    name = 'aumix'
    version = '2.8'

    buildRequires = [ ]
    
    def setup(r):
        r.addArchive('http://www.jpj.net/~trevor/aumix/%(name)s-%(version)s.tar.bz2')
        r.Configure('--without-gtk --without-gpm --without-gtk1')
        r.Make()
        r.MakeInstall()

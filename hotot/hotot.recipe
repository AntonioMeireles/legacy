class Hotot(PackageRecipe):

    name = 'hotot'
    version = '10272010'

    buildRequires = ['python-setuptools:python','gettext:runtime', 'intltool:runtime', 'python:devel', 'pywebkitgtk:python']

    def setup(r):
        r.addMercurialSnapshot("https://hotot.googlecode.com/hg/")
        r.PythonSetup(data='--install-data=%(prefix)s')

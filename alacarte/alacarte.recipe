class AlaCarte(AutoPackageRecipe):

    buildRequires = ['gnome-menus:python', 'pygtk:python']

    name = 'alacarte'
    version = '0.9'

    def unpack(r):
        r.addArchive('http://dev.realistanew.com/%(name)s/releases/%(version)s/%(name)s-%(version)s.tar.gz')

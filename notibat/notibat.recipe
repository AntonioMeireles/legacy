class Notibat(PackageRecipe):
    buildRequires = [ 'gnome-panel:devel' ]

    name = 'notibat'
    version = '0.8.1.1' # latest version in GNOME CVS
    
    def setup(r):
	r.addArchive('%(name)s-%(version)s.tar.gz')
        r.Configure()
        r.Make()
        r.MakeInstall()


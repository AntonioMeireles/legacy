class EdsFeed(PackageRecipe):
    buildRequires = [ 'libgalago:devel','evolution-data-server:devel' ]

    name = "eds-feed"
    version = "0.3.2"
    
    def setup(r):
        r.addArchive('http://www.galago-project.org/files/releases/source/%(name)s/%(name)s-%(version)s.tar.gz')
	r.Configure()
	r.Make()
	r.MakeInstall()

class Raptor(AutoPackageRecipe):

    name = 'raptor'
    version = '1.4.16'

    buildRequires = [
        'curl:devel', 'flex:runtime', 'libxml2:devel', 'libxslt:devel',
        'openssl:devel', 'perl:runtime', 'zlib:devel'
    ]

    def unpack(r):

        r.addArchive('http://download.librdf.org/source/')


loadSuperClass('xorgpackage.recipe')
class LibXtst(XorgPackageRecipe):
    name = 'libXtst'
    version = '1.0.1'
    component = 'lib'

    buildRequires = [ 'libXext:devel', 'recordproto:devel', 'xextproto:devel' ]

    def setup(r):
        XorgPackageRecipe.setup(r)

        r.Requires('recordproto:devel', '%(libdir)s/pkgconfig/xtst.pc')

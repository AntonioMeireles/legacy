class Numactl(CPackageRecipe):
    name = 'numactl'
    version = '2.0.3'

    buildRequires = []

    def setup(r):
        r.addArchive('ftp://oss.sgi.com/www/projects/libnuma/download/')
        r.Make()
        r.MakeInstall('prefix=%(destdir)s%(prefix)s')

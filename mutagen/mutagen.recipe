class Mutagen(PackageRecipe):
    buildRequires = [ 'python-setuptools:python' ]
    name = 'mutagen'
    version = '1.1'

    def setup(r):
        r.addArchive('http://www.sacredchao.net/~piman/software/%(name)s-%(version)s.tar.gz')
        r.PythonSetup()

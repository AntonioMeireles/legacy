class Rasqal(AutoPackageRecipe):

    name = 'rasqal'
    version = '0.9.21'

    buildRequires = [
        'flex:runtime', 'gmp:devel', 'libxml2:devel', 'm4:runtime',
        'mpfr:devel', 'pcre:devel', 'perl:runtime', 'pkgconfig:devel',
        'raptor:devel'
    ]

    def unpack(r):

        r.addArchive('http://download.librdf.org/source/')


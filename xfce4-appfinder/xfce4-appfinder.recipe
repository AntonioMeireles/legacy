class xfce4Appfinder(XfcePackageRecipe):

    name = 'xfce4-appfinder'
    version = '4.8.0'

    XfcePackageRecipe.buildRequires.extend([
        'libxfce4util:devel', 'garcon:devel', 'libxfce4ui:devel',
        'xfconf:devel', 'desktop-file-utils:runtime', 'Thunar:devel'
    ])

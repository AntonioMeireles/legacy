class Smokeqt(CPackageRecipe):
    name = 'smokeqt'
    version = '4.8.3'
 
    buildRequires = []
 
    def setup(r):
        r.addArchive('mirror://kde//stable/%(version)s/src/')
        r.CMake()
        r.Make()
        r.MakeInstall()

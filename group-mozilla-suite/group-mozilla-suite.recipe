class Dist(GroupRecipe):
    name = 'group-mozilla-suite'
    version = '1.7.8'
    #depCheck = True

    def setup(r):
        r.setLabelPath('conary.foresightlinux.com@fl:desktop', 'conary.rpath.com@rpl:devel', 'contrib.rpath.com@rpl:devel')

        r.addTrove("mozilla")
        r.addTrove("mozilla-chat", source='mozilla')
        r.addTrove("mozilla-dom-inspector", source='mozilla')
        r.addTrove("mozilla-js-debugger", source='mozilla')
        r.addTrove("mozilla-mail", source='mozilla')
        r.addTrove("mozilla-nspr", source='mozilla')
        r.addTrove("mozilla-nss", source='mozilla')

class isocodes(AutoPackageRecipe):
    buildRequires=[ 'glibc:devel','gettext:devel','python:runtime',
        'PyXML:python', 'gawk:runtime', 'gettext:runtime' ]

    name = "iso-codes"
    version = "0.46"

    def unpack(r):
        r.addArchive("http://distro.ibiblio.org/pub/linux/distributions/debian/pool/main/i/%(name)s/iso-codes_%(version)s.orig.tar.gz")

class SReadahead(CPackageRecipe):

    name = 'sreadahead'
    version = '0.02'

    buildRequires = [ ] 

    def setup(r):
        r.addArchive('http://www.moblin.org/sites/all/files/%(name)s-%(version)s.tar.gz')
        r.addPatch('sreadahead-0.02-make.patch')
        r.Make()
        r.MakeInstall()

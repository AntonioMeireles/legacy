class eina(AutoPackageRecipe):
    name = 'eina'
    version = "1.2.0"

    buildRequires = ['pkgconfig:devel', 'automake:runtime', 'autoconf:runtime', 'libtool:runtime', 'doxygen:runtime']
    def unpack(r):
        r.addArchive("http://download.enlightenment.org/releases/eina-%(version)s.tar.gz")

class ForesightContexts(PackageRecipe):
    name = 'foresight-contexts'
    version = '0.1'
 
    buildRequires = []
 
    def setup(r):
        r.addSource('rmake.conf', dest='/etc/foresight/development/', mode=0644)
        r.addSource('conary.conf', dest='/etc/foresight/development/', mode=0644)

        if Arch.x86: pass

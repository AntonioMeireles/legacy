class PythonINIParse(CPackageRecipe):
    name = 'python-iniparse'
    version = '0.3.1'

    buildRequires = ['python-setuptools:python', 'python:devel']

    def setup(r):
        r.macros.name = r.macros.name.replace('python-', '')
        r.addArchive('http://iniparse.googlecode.com/files/')
        r.PythonSetup()

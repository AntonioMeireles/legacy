loadSuperClass('rpmpackage')
class LibAIO(RPMPackageRecipe, CPackageRecipe):
    name = 'libaio'
    version = '0.3.106'
    isRHEL = True
    distroVersion = '5Server'
    rpmRelease = '3.2'

    def setup(r):
        r.unpack()

        r.Make()
        r.MakeInstall('prefix=%(destdir)s/%(prefix)s')

class Fceu(AutoPackageRecipe):
    name = 'fceu'
    version = '0.98.12'
 
    buildRequires = [ 'SDL:devel', 'zlib:devel' ]
 
    def unpack(r):
        r.addArchive('http://umn.dl.sourceforge.net/sourceforge/fceultra/'
                     '%(name)s-%(version)s.src.tar.bz2')


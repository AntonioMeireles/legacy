class xfce4_icon_theme(AutoPackageRecipe):

    buildRequires = [ 'pkgconfig:devel', 'gtk:runtime' ]

    name = 'xfce4-icon-theme'
    version = '4.2.3'

    def unpack(r):
        r.addArchive('http://www.us.xfce.org/archive/xfce-%(version)s/src/%(name)s-%(version)s.tar.gz')


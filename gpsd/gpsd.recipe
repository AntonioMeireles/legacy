class GPSD(AutoPackageRecipe):
    name = 'gpsd'
    version = '2.34'

    buildRequires = ['libstdc++:devel', 'ncurses:devel']

    def unpack(r):
        r.addArchive('http://download.berlios.de/%(name)s/')

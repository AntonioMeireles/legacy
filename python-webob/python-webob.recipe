loadSuperClass('pypipackage')
class WebOb(PyPiPackage):
    name = 'python-webob'
    pyname = 'WebOb'
    version = '1.1.1'
    buildRequires = [ 'unzip:runtime', ]

    # temporary workaround for not finding the package
    def getPackageURL(r):
        return 'http://pypi.python.org/packages/source/W/WebOb/WebOb-1.1.1.zip'

    def install(r):
        r.SetModes('%(sitepkgs)s/WebOb*/*', 0644)

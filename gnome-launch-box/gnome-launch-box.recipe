class GnomeLaunchBox(PackageRecipe):
   buildRequires = [ 'gtk:devel', 'evolution-data-server:devel', 'gnome-menus:devel' ]
	
   name = "gnome-launch-box"
   version = "0.1"
   def setup(r):
	r.addArchive('http://ftp.imendio.com/pub/imendio/%(name)s/src/%(name)s-%(version)s.tar.bz2')
	r.Configure()
	r.Make()
	r.MakeInstall()

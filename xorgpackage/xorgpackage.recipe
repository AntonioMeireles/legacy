loadSuperClass('xorgcpackage.recipe')
class XorgPackageRecipe(XorgCPackageRecipe):
    name = 'xorgpackage'
    version = '1'

    buildRequires = [ 'inputproto:devel', 'kbproto:devel', 'libX11:devel',
        'libXau:devel', 'libXdmcp:devel', 'xproto:devel' ]

    def setup(r):
        if r.__class__.__name__ == 'XorgPackageRecipe':
            return

        XorgCPackageRecipe.setup(r)

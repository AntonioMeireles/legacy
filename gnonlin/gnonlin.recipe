class Gnonlin(AutoPackageRecipe):
    name = 'gnonlin'
    version = '0.10.10'

    buildRequires = [ 'check:devel', 'glib:devel', 'gst-plugins-base:devellib', 
'gstreamer:devel', 'libxml2:devel', 'pkgconfig:devel', 'zlib:devel' ]

    def unpack(r):
        r.addArchive('http://gstreamer.freedesktop.org/src/%(name)s/')

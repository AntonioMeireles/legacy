class LibVNCServer(AutoPackageRecipe):

    name = 'libvncserver'
    version = '0.9.1'

    buildRequires = [
        'libjpeg:devel', 'zlib:devel'
    ]

    def unpack(r):

        r.macros.archive_name = 'LibVNCServer'
        r.addArchive('mirror://sourceforge/libvncserver/')


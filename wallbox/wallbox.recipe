class Wallbox(PackageRecipe):

    name = 'wallbox'
    version = '2010.01.28'

    buildRequires = ['python-setuptools:python', 'dbus-python:python', 'pyfacebook:python', 'pygobject:python', 'pygtk:python', 'python:devel']

    def setup(r):
        r.addGitSnapshot('http://github.com/yurenju/wallbox.git')
        r.PythonSetup()

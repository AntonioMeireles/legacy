class Aiccu(CPackageRecipe):
    name = 'aiccu'
    version = '20070115'
 
    buildRequires = [
        'chkconfig:runtime',
        'gnutls:devel',
      ]
 
    def setup(r):
        r.addArchive('http://www.sixxs.net/archive/sixxs/aiccu/unix/aiccu_%(version)s.tar.gz')
        r.Make()
        r.MakeInstall('RPM_BUILD_ROOT=trickery')

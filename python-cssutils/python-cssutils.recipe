class PythonCssUtils(PackageRecipe):
    name = 'python-cssutils'
    version = '0.9.6b1'

    buildRequires = ['python-setuptools:python', 'python:devel', 'unzip:runtime']

    def setup(r):
        r.addArchive('http://cssutils.googlecode.com/files/cssutils-%(version)s.zip')
        r.PythonSetup()

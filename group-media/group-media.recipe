class Dist(GroupRecipe):
    name = 'group-media'
    version = '0.8.2.free.1'
    #depCheck = True

    def setup(r):
        r.setLabelPath('conary.foresightlinux.com@fl:desktop', 'conary.rpath.com@rpl:devel', 'contrib.rpath.com@rpl:devel')
        r.addTrove("alsa-lib")
        r.addTrove("alsa-utils")
        r.addTrove("arts")
        r.addTrove("audiofile")
        r.addTrove("banshee")
        r.addTrove("cdparanoia")
        r.addTrove("cowbell")
        r.addTrove("esound")
        r.addTrove("flac")
        r.addTrove("goobox")
        r.addTrove("gst-plugins")
        r.addTrove("gstreamer")
        r.addTrove("ipod-sharp")
        r.addTrove("jack")
        r.addTrove("libao")
        r.addTrove("libid3tag")
        r.addTrove("libipoddevice")
        r.addTrove("libmusicbrainz")
        r.addTrove("libogg")
        r.addTrove("libsndfile", "contrib.rpath.com@rpl:devel")
        r.addTrove("libtheora")
        r.addTrove("libvorbis")
        r.addTrove("mplayerplug-in")
        r.addTrove("speex")
        r.addTrove("taglib")
        r.addTrove("totem")
        r.addTrove("vorbis-tools")

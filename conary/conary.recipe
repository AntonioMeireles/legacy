class Conary(RedirectRecipe):
    name = 'conary'
    version = '0'

    def setup(r):
        r.addRedirect('conary', 'conary.rpath.com@rpl:1')
        r.addRedirect('conary-repository', 'conary.rpath.com@rpl:1',
                       fromTrove='conary-repository')
        r.addRedirect('conary-build', 'conary.rpath.com@rpl:1',
                       fromTrove='conary-build')

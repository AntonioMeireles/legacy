    name = 'telepathy-gabble'
    version = '0.8.1'

    buildRequires = ['dbus-glib:runtime', 'loudmouth:devel',
'telepathy-glib:devel', 'e2fsprogs:devel', 'gnutls:devel', 'libgcrypt:devel',
'libgpg-error:devel', 'libsoup:devel']

    externalArchive ='http://telepathy.freedesktop.org/releases/%(name)s/'

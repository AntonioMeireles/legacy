loadRecipe('gnomepackage.recipe')
class TelepathyGabble(GnomePackageRecipe):

    name = 'telepathy-gabble'
    version = '0.8.7'

    buildRequires = [
        'dbus-glib:runtime',
        'e2fsprogs:devel',
        'gnutls:devel',
        'libgcrypt:devel',
        'libgpg-error:devel',
        'libsoup:devel',
        'loudmouth:devel',
        'telepathy-glib:devel',
        ]

    externalArchive ='http://telepathy.freedesktop.org/releases/%(name)s/'

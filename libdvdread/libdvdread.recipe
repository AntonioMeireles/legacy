class Libdvdread(AutoPackageRecipe):

    name = 'libdvdread'
    version = '0.9.7'
    
    buildRequires = []

    def unpack(r):
        r.addArchive('http://www.dtek.chalmers.se/groups/dvd/dist/%(name)s-%(version)s.tar.gz')
        r.addPatch('inttypes_h.patch')



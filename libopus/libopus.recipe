class Libopus(AutoPackageRecipe):
    name = 'libopus'
    version = '1.0.1'

    buildRequires = [
        'doxygen:runtime',
    ]

    def unpack(r):
        r.addArchive('http://downloads.xiph.org/releases/opus/opus-%(version)s.tar.gz')

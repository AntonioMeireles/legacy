class PyWBEM(PackageRecipe):
    name = 'pywbem'
    version = '0.7.0'

    buildRequires = [
        'python:devel',
        'python-setuptools:python',
    ]

    def setup(r):
        r.addArchive('mirror://sourceforge/%(name)s/')
        r.PythonSetup()

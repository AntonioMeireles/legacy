class KibaDbusPlugins(AutoPackageRecipe):
    name = 'kiba-dbus-plugins'
    version = '0.2'
 
    buildRequires = ['dbus-python:python', 'pygobject:python']
 
    def unpack(r):
        r.addSvnSnapshot('https://kibadock.svn.sourceforge.net/svnroot/kibadock/trunk/kiba-dbus-plugins/')
    def configure(r):
        r.Run('./autogen.sh')
	r.Configure()


class Dist(GroupRecipe):
    name = 'group-internet'
    version = '0.9.1'
    #depCheck = True

    def setup(r):
        r.setLabelPath('foresight.rpath.org@fl:desktop', 'conary.rpath.com@rpl:1', 'contrib.rpath.org@rpl:devel')
        r.addTrove("blam")
        r.addTrove("firefox")
        r.addTrove("firefox-rBuilder-search")
	r.addTrove("gaim")
	r.addTrove("gftp")
        #r.addTrove('group-mozilla-suite', byDefault=False)
	r.addTrove("quagga")
	r.addTrove("rp-pppoe")
	r.addTrove("wget")
	r.addTrove("wvdial")
	r.addTrove("xchat-gnome")


class Dist(GroupRecipe):
    name = 'group-internet'
    version = '0.8.2.1'
    #depCheck = True

    def setup(r):
        r.setLabelPath('conary.foresightlinux.com@fl:desktop', 'conary.rpath.com@rpl:devel', 'contrib.rpath.com@rpl:devel')
        r.addTrove("blam")
        r.addTrove('epiphany', byDefault=False)
        r.addTrove("firefox")
	r.addTrove("gaim")
	r.addTrove("gftp")
        r.addTrove('group-mozilla-suite', byDefault=False)
	r.addTrove("quagga")
	r.addTrove("rp-pppoe")
	r.addTrove("wget")
	r.addTrove("wvdial")
	r.addTrove("xchat-gnome")


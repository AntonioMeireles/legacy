loadSuperClass('gnomepackage')
class Gegl(GnomePackageRecipe):

    name = 'gegl'
    version = '0.0.12'

    buildRequires = ['OpenEXR:devel', 'SDL:devel',  'babl:devel', 
'enscript:runtime', 'font-adobe-75dpi:data', 'fontconfig:devel', 
'fontconfig:runtime', 'gnome-vfs:devel', 'ilmbase:devel', 'libjpeg:devel', 
'librsvg:devel', 'libstdc++:devel', 'ruby:runtime' ]

    externalArchive = 'ftp://ftp.gimp.org/pub/gegl/0.0/'

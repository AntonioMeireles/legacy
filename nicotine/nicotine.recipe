class Nicotine(PackageRecipe): 
	name = 'nicotine'
	version = '1.0.8'

	buildRequires = ['python-setuptools:python', 'gnome-python:python', 'pygobject:python', 'pygtk:python']

	def setup(r):	
    	    r.addArchive('http://nicotine.thegraveyard.org/')
	    r.PythonSetup()	

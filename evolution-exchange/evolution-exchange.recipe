loadRecipe('gnomepackage.recipe')
class EvolutionExchange(GnomePackageRecipe):
    name = 'evolution-exchange'
    version = '2.8.0'

    buildRequires = []

    # add krb5 support
    if Use.krb:
        extraConfig = ' --with-krb5=/usr/kerberos'
        buildRequires.append('krb5:devel')
    else:
        extraConfig = ' --without-krb5'


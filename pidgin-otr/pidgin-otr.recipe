class PidginOtr(AutoPackageRecipe):
    name = 'pidgin-otr'
    version = '3.0.0'
 
    buildRequires = [ 'libotr:devel', 'libgcrypt:devel', 'libgpg-error:devel','pkgconfig:devel']
 
    def unpack(r):
        r.addArchive('http://www.cyberdyne.org/~icebrkr/files/%(name)s-%(version)s.tar.gz')

class IpodSharp(PackageRecipe):
   buildRequires = [ 'mono:devel', 'libipoddevice:devellib' ]
	
   name = "ipod-sharp"
   version = "20050719"
   def setup(r):
	r.addArchive('%(name)s.tar.bz2')
	#r.addArchive('http://www.snorp.net/files/packages/%(name)s-%(version)s.tar.gz')
	r.Run('./autogen.sh')
	r.Configure()
	r.Make()
	r.MakeInstall()

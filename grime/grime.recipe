loadRecipe('gnomepackage.recipe')
class gDesklets(GnomePackageRecipe):
    buildRequires = [ "glibc:devel" ]
    name = "grime"
    version = "0.5.3.1"

    def setup(r):
        r.addArchive('http://jaist.dl.sourceforge.net/sourceforge/grime/%(name)s-%(version)s.tar.bz2')
        r.Configure()
        # r.Make()
        r.MakeInstall()

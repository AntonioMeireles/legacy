loadRecipe('cpanpackage')
class PerlGD(CPANPackageRecipe, CPackageRecipe):
    name = 'perl-GD'
    module = 'GD'
    version = '2.41'

    buildRequires = [
        'fontconfig:devel',
        'freetype:devel',
        'gd:devel',
        'libjpeg:devel',
        'libpng:devel',
        'libX11:devel',
        'libXpm:devel',
        'perl:devel',
        'zlib:devel',
    ]

class Xfce4DictPlugin(AutoPackageRecipe):
    name = 'xfce4-dict-plugin'
    version= "20080715" 
    buildRequires = ['xfce4-dev-tools:data','xfce4-dev-tools:runtime','autoconf:runtime','atk:devel', 'cairo:devel', 'desktop-file-utils:runtime', 'expat:devel', 'fontconfig:devel', 'freetype:devel', 'gettext:runtime', 'glib:devel', 'gtk:devel', 'gtk:runtime', 'intltool:runtime', 'libX11:devel', 'libXrender:devel', 'libpng:devel', 'libxcb:devel', 'libxfce4util:devel', 'libxfcegui4:devel', 'pango:devel', 'pixman:devel', 'pkgconfig:devel', 'xcb-util:devel', 'xfce4-panel:devel', 'zlib:devel','libtool:runtime','automake:runtime','intltool:runtime','gettext:devel','autoconf:data','autoconf:runtime','intltool:devel','intltool:data']
 
    def unpack(r):
        r.addSvnSnapshot('http://svn.xfce.org/svn/goodies/xfce4-dict/trunk')
        r.Configure(configureName="autogen.sh")

class SDL_sound(PackageRecipe):
    buildRequires = [ 'glibc:devel','SDL:devel',
        'binutils:runtime', 'flac:devel', 'gawk:runtime',
        'libvorbis:devel' ]

    name = "SDL_sound"
    version = "1.0.1"

    def setup(r):
        r.addArchive('http://icculus.org/SDL_sound/downloads/%(name)s-%(version)s.tar.gz')
        r.Configure()
        r.Make()
        r.MakeInstall()


loadSuperClass('gnomepackage.recipe')
class GnomeBluetooth(GnomePackageRecipe):
    buildRequires = [ ]
    name = "gnome-bluetooth"
    version = "0.5.1"

    externalArchive = 'http://downloads.usefulinc.com/gnome-bluetooth/%(name)s-%(version)s.tar.gz'

    def unpack(r):
	GnomePackageRecipe.unpack(r)
	r.addPatch("gnome-bluetooth-0.5.1-gnomepython.patch")

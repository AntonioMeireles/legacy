class SblimCmpiDns(AutoPackageRecipe):
    name = 'sblim-cmpi-dns'
    version = '0.5.5'

    buildRequires = [
        'flex:runtime',
        'libstdc++:devel',
        'sblim-cmpi-devel:devel',
        'sblim-sfcb:runtime', # For the tag spec
        'sblim-tools-libra:devel',
    ]

    def unpack(r):
        r.addArchive('mirror://sourceforge/sblim/')

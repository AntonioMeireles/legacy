class dtach(CPackageRecipe):
    name = 'dtach'
    version = '0.7'

    def setup(r):
        r.addArchive('http://easynews.dl.sourceforge.net/sourceforge/%(name)s/%(name)s-%(version)s.tar.gz')
        r.Configure()
        r.Make()
        r.Install('dtach', '%(bindir)s/')
        r.Install('dtach.1', '%(mandir)s/man1/')

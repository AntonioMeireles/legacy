class Libbtctl(AutoPackageRecipe):
    buildRequires = [ "autoconf", "glib:devel", "gtk:doc", "openobex:devel", "bluez-libs:devel", "python", "pygtk:devel" ]

    name = "libbtctl"
    version = "0.8.0"

    def unpack(r):
        r.addArchive("http://ftp.gnome.org/pub/gnome/sources/libbtctl/0.8/%(name)s-%(version)s.tar.gz")

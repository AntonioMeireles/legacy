loadRecipe('gnomepackage.recipe', label='conary.rpath.com@rpl:devel')
class Blam(GnomePackageRecipe):

    buildRequires = [ 'gtk:devel', 'gtk-sharp:devellib',
                        'gecko-sharp:devellib', 'mono:devel',
			'GConf:devel' ]

    name = 'blam'
    version = '1.8.2'

    def unpack(r):
        GnomePackageRecipe.unpack(r)
        r.addSource('collection.xml')


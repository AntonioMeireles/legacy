loadRecipe('gnomepackage.recipe')
class LibGdamm(GnomePackageRecipe):

    name = 'libgdamm'
    version = '2.9.5'

    buildRequires = ['glibmm:devel', 'libgda:devel', 'libsigc++:devel', 
                     'libstdc++:devel', 'm4:runtime']

    def unpack(r):
	GnomePackageRecipe.unpack(r)

    def install(r):
       GnomePackageRecipe.install(r)

class Openldap(RedirectRecipe):
    name = 'openldap'
    version = '0'

    def setup(r):
        r.addRedirect('openldap', 'conary.rpath.com@rpl:1')
        r.addRedirect('openldap-clients', 'conary.rpath.com@rpl:1', fromTrove='openldap-clients')
        r.addRedirect('openldap-servers', 'conary.rpath.com@rpl:1', fromTrove='openldap-servers')

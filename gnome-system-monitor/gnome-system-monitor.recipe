loadRecipe('gnomepackage.recipe')
class GnomePackage(GnomePackageRecipe):
    
    buildRequires = GnomePackageRecipe.buildRequires
    buildRequires.extend( ['libgnomeui:devel', 'libgtop:devel', 'libwnck:devel',
                           'pango:devel', 'gtk:devel', 'startup-notification:devel',
                           'intltool:runtime', 'gettext:runtime'] )
               
    name = 'gnome-system-monitor'
    version = '2.10.1'

class Cairo(AutoPackageRecipe):
    name = 'cairo'
    version = '1.0.0'

    buildRequires = [ 'libpixman:devel', 'fontconfig:devel', 
			'freetype:devel', 'libpng:devel', 
			'xorg-x11:devel', 'zlib:devel' ]
    
    def unpack(r):
	r.addArchive('http://cairographics.org/releases/%(name)s-%(version)s.tar.gz')

class LibgalagoGtk(PackageRecipe):
    buildRequires = [ 'libgalago:devel','gtk:devel' ]

    name = "libgalago-gtk"
    version = "0.3.3"
    
    def setup(r):
        r.addArchive('http://www.galago.info/files/releases/source/%(name)s-%(version)s.tar.gz')
	r.Configure()
	r.Make()
	r.MakeInstall()

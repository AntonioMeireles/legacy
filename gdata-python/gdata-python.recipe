class GdataPython(BuildPackageRecipe):
    name = 'gdata-python'
    version = '1.0.10'
 
    buildRequires = ['cElementTree:python', 'python:devel', 'python-setuptools:python']
 
    def setup(r):
        r.addArchive('http://gdata-python-client.googlecode.com/files/gdata.py-%(version)s.latest.tar.gz')
        r.PythonSetup()

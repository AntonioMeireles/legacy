class PythonNumeric(PackageRecipe):
   buildRequires = [ 'python-setuptools:python', 'Pyrex:python',
        'coreutils:runtime', 'glibc:devel','python-numeric:python']
        
   name = "numpy"
   version = "1.0.1"
   def setup(r):
        r.addArchive('http://dl.sf.net/numpy/%(name)s-%(version)s.tar.gz')
        r.PythonSetup()

class Numpy(CPackageRecipe):        
   name = "numpy"
   version = "1.2.1"
   buildRequires = ['python-ctypes:python', 'python-numeric:python', 'python:devel', 'python-setuptools:python']
   def setup(r):
        r.addArchive('http://kent.dl.sourceforge.net/sourceforge/numpy/%(name)s-%(version)s.tar.gz')
	r.PythonSetup(data='--install-data=%(prefix)s')

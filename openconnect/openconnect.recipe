class OpenConnect(PackageRecipe):
    name = 'openconnect'
    version = '2.25'
    buildRequires = ['glibc:devel', 'libxml2:devel', 'make:runtime', 'openssl:devel', 'zlib:devel', 'gcc-c++:runtime', 'gcc:runtime', 'gcc:devel']
    def setup(r):
        r.addArchive('ftp://ftp.infradead.org/pub/openconnect/')
        r.Make()
        r.MakeInstall()

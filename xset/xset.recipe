loadSuperClass('xorgpackage')
class Xset(XorgPackageRecipe):
    name = 'xset'
    version = '1.0.3'
    component = 'app'

    # need a Use flag for xprint
    buildRequires = [ 'fontcacheproto:devel', 'libICE:devel', 'libSM:devel',
        'libXext:devel', 'libXfontcache:devel', 'libXmu:devel', 'libXt:devel',
        'libXxf86misc:devel' ]

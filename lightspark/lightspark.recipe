class Lightspark(CPackageRecipe):
    name = 'lightspark'
    version = '2010.06.06'
 
    buildRequires = []
 
    def setup(r):
        r.addGitSnapshot('http://github.com/alexp-sssup/lightspark.git')
        r.CMake()
        r.Make()
        r.MakeInstall()

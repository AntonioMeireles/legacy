class iconfig(BuildPackageRecipe):
    name = 'iconfig'
    version = 'tip'

    buildRequires = [ 'python:devel', 'conary:python', 'Genshi:python' ]

    def setup(r):
        r.addMercurialSnapshot('http://hg.rpath.com/iconfig')
        r.MakeInstall()
        r.InitialContents('/var/log/iconfig')

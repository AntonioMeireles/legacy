class Genshi(PackageRecipe):
    name = 'Genshi'
    version = '0.4.1'

    buildRequires = [ 'python-setuptools:python' ]

    def setup(r):
        r.addArchive('http://ftp.edgewall.com/pub/genshi/')

        r.PythonSetup()

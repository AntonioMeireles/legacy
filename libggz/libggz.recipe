class GgzLibs(AutoPackageRecipe):
    name = 'libggz'
    version = '0.0.14'
    buildRequires = ['libgcrypt:devel']


    def unpack(r):
        r.addArchive('http://ftp.ggzgamingzone.org/pub/ggz/%(version)s/',
                     '%(name)s-%(version)s.tar.gz')

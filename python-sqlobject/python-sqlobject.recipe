class pythonsqlobject(PackageRecipe):
 
    name = "python-sqlobject"
    version = "0.6.1"
 
    def setup(r):
        r.addArchive("http://puzzle.dl.sourceforge.net/sourceforge/sqlobject/SQLObject-%(version)s.tar.gz")
        r.mainDir('SQLObject-%(version)s')
        r.Run("python setup.py build")
        r.Run('python setup.py install --root=%(destdir)s')

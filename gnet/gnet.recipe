class GNet(AutoPackageRecipe):
    name = 'gnet'
    version = '2.0.8'

    buildRequires = [ 'glib:devel', 'pkgconfig:devel', 'file:runtime', ]

    def unpack(r):
        r.addArchive('http://ftp.gnome.org/pub/GNOME/sources/%(name)s/2.0/%(name)s-%(version)s.tar.bz2')

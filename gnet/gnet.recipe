class GNet(PackageRecipe):
    name = 'gnet'
    version = '2.0.7'

    buildRequires = [ 'glib:devel' ]

    def setup(r):
        r.addArchive('http://www.gnetlibrary.org/src/gnet-%(version)s.tar.gz')

        r.Configure()
        r.Make()
        r.MakeInstall()


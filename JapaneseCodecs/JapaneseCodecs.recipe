class Japanesecodecs(PackageRecipe):
    name = 'JapaneseCodecs'
    version = '1.4.11'

    buildRequires = ['python-setuptools:python',
		'glibc:devel']

    def setup(r):
	r.addArchive('http://ftp.python.jp/pub/%(name)s/%(name)s-%(version)s.tar.gz')
	r.PythonSetup()

class libdaemon(PackageRecipe):
    buildRequires = [ 'gcc:devel','lynx:devel' ]

    name = "libdaemon"
    version = "0.7"

    def setup(r):
        r.addArchive("http://www.stud.uni-hamburg.de/users/lennart/projects/libdaemon/%(name)s-%(version)s.tar.gz")
	r.Configure()
	r.Make()
	r.MakeInstall()

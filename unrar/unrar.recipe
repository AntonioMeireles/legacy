class UnRar(PackageRecipe):
    name = 'unrar'
    version = '2.71'
    
    buildRequires = [  ]   

    def setup(r):
	r.addArchive('ftp://sunsite.unc.edu/pub/Linux/utils/compress/%(name)s-%(version)s.tar.gz')
	r.Replace('\/usr\/local\/bin','$(DESTDIR)/usr/bin','Makefile')
	r.MakeDirs('%(bindir)s')
	r.Make()
	r.MakeInstall()

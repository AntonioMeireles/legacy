class Dist(GroupRecipe):
    name = 'group-xorg'
    version = '6.8.2'
    #depCheck = True  

    def setup(r):
        r.setLabelPath('conary.foresightlinux.com@fl:desktop', 'conary.rpath.com@rpl:devel', 'contrib.rpath.com@rpl:devel')
        r.addTrove("xorg-x11")
        r.addTrove("xorg-x11-fonts", source='xorg-x11')
        r.addTrove("xorg-x11-tools", source='xorg-x11')
        r.addTrove("xorg-x11-xfs", source='xorg-x11')

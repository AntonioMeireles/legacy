class Xorg(GroupRecipe):
    name = 'group-xorg'
    version = '6.8.2'
    #depCheck = True  

    def setup(r):
        r.setLabelPath('foresight.rpath.org@fl:desktop', 'conary.rpath.com@rpl:1', 'contrib.rpath.org@rpl:devel')
        r.addTrove("xorg-x11")
        r.addTrove("xorg-x11-fonts", source='xorg-x11')
        r.addTrove("xorg-x11-tools", source='xorg-x11')
        r.addTrove("xorg-x11-xfs", source='xorg-x11')

class MonoDevelop(PackageRecipe):
    buildRequires = [ 'mono:runtime', 'gtk-sharp2:devellib' ]

    name = "stetic"
    version = "44023"
    
    def setup(r):
        r.addArchive("stetic-%(version)s.tar.bz2")
	r.mainDir('%(name)s')
	r.Configure()
	r.Make()
	r.MakeInstall()
	r.NonBinariesInBindirs(exceptions=['%(bindir)s/.*\.exe'])

loadSuperClass('gnomepackage.recipe')
class SoundJuicer(GnomePackageRecipe):
    name = 'sound-juicer'
    version = '2.10.1'

    buildRequires = GnomePackageRecipe.buildRequires
    buildRequires.extend(['libmusicbrainz:devel',
                          'libgnomeui:devel',
                          'glib:devel',
                          'gstreamer:devel',
                          'GConf:devel',
                          'scrollkeeper:runtime',
                          'gcc:runtime',
                          'autoconf:runtime'])

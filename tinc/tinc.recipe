class Tinc(AutoPackageRecipe):
    name = 'tinc'
    version = '1.0.19'
 
    buildRequires = [
        'lzo:devel',
        'openssl:devel',
        'zlib:devel',
      ]
 
    def unpack(r):
        r.addArchive('http://www.tinc-vpn.org/packages/')

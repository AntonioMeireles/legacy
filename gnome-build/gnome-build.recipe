loadSuperClass('gnomepackage.recipe')
class gnomebuild(GnomePackageRecipe):
    name = "gnome-build"
    version = "0.1.3"

    buildRequires = ['GConf:devel', 'ORBit2:devel', 'atk:devel', 'cairo:devel',
'gdl:devel', 'fontconfig:devel', 'freetype:devel', 'gettext:runtime',
'glib:devel', 'glib:runtime', 'gnome-keyring:devel', 'gnome-vfs:devel',
'libart_lgpl:devel', 'libbonobo:devel', 'libbonoboui:devel', 'libglade:devel',
'libgnome:devel', 'libgnomecanvas:devel', 'libgnomeui:devel', 'libpng:devel',
'libxml2:devel', 'pango:devel', 'perl:lib', 'perl:runtime', 'perl-XML-Parser:perl', 'popt:devel']

    externalArchive =("http://ftp.gnome.org/pub/GNOME/sources/%(name)s/0.1/%(name)s-%(version)s.tar.bz2")

loadSuperClass('gnomepackage.recipe')
class GNUDoku(GnomePackageRecipe):
    buildRequires = [ ]
    name = "GNUDoku"
    version = "0.92"

    externalArchive = 'http://icculus.org/~jcspray/%(name)s/%(name)s-%(version)s.tar.gz'

    def unpack(r):
	GnomePackageRecipe.unpack(r)
        r.addSource('Makefile')
        #r.addSource('gnome-splash.png', dir='gnome-session')
        r.Make()
        r.MakeInstall()

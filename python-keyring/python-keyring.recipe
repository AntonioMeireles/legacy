class PythonKeyring(PackageRecipe):

    name = 'python-keyring'
    version = '0.2+c83'

    buildRequires = ['python-setuptools:python', 'dbus:devel', 'glib:devel', 'libgnome-keyring:devel', 'python:devel']

    def setup(r):
        r.addMercurialSnapshot('http://bitbucket.org/kang/python-keyring-lib/')
        r.PythonSetup()

class Xsp(PackageRecipe):

    name = "xsp"
    version = "1.1.9.1"

    buildRequires = [ 'gawk:runtime', 'mono:runtime', 'pkgconfig:devel' ]

    def setup(r):
        r.addArchive('http://go-mono.com/sources/%(name)s/%(name)s-%(version)s.tar.gz')

        r.Configure()
        r.Make()
        r.MakeInstall()

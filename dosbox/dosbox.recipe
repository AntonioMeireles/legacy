class Dosbox(AutoPackageRecipe):
    name = 'dosbox'
    version = '0.72'
 
    buildRequires = ['Mesa:devel', 'SDL:devel', 'SDL_net:devel', 'alsa-lib:devel', 'libpng:devel', 'libstdc++:devel', 'zlib:devel']
 
    def unpack(r):
        r.addArchive('mirror://sourceforge/dosbox/%(name)s-%(version)s.tar.gz')

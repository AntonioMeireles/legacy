class Fio(CPackageRecipe):
    name = 'fio'
    version = '1.51'

    buildRequires = ['libaio:devel']

    def setup(r):
        r.addGitSnapshot('git://git.kernel.dk/fio.git',
                         tag='%(name)s-%(version)s')
        r.Make()
        r.MakeInstall()

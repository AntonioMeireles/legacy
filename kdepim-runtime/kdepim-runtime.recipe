loadSuperClass('kdecorepackage')
class kdePIMRuntime(kdeCorePackageRecipe):

    name = 'kdepim-runtime'
    version = '4.4.10'

    buildRequires = [
        'akonadi:devel', 'libxml2:devel', 'strigi:devel'
    ]

    def unpack(r):

        r.addArchive('mirror://kde/stable/kdepim-%(version)s/src/')


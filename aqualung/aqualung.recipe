class Aqualung(AutoPackageRecipe):
    name = 'aqualung'
    version = '0.9beta10'
 
    buildRequires = ['alsa-lib:devel', 'atk:devel', 'bzip2:devel', 
	'cairo:devel', 'expat:devel', 'fontconfig:devel', 'freetype:devel', 
	'glib:devel', 'gtk:devel', 'jack:devel', 'libX11:devel', 
	'libXrender:devel', 'libogg:devel', 'libpng:devel', 
	'libsamplerate:devel', 'libsndfile:devel', 'libstdc++:devel', 
	'libusb:devel', 'libvorbis:devel', 'libxcb:devel', 'libxml2:devel', 
	'pango:devel', 'pixman:devel', 'pkgconfig:devel', 'xcb-util:devel', 
	'zlib:devel', 'ffmpeg:devel', 'flac:devel', 'lame:devel', 'libcdio:devel', 'libmad:devel']
 
    def unpack(r):
        r.addArchive('mirror://sourceforge/aqualung/%(name)s-%(version)s.tar.gz')

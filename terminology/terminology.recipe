class Terminology(AutoPackageRecipe):
    name = 'terminology'
    version = '0.2.0'

    buildRequires = [
        'dbus:devel', 
        'desktop-file-utils:runtime', 
        'ecore:devel', 
        'e-dbus:devel', 
        'edje:devel',
        'edje:runtime', 
        'eet:devel', 
        'efreet:devel', 
        'eina:devel', 
        'elementary:devel', 
        'embryo:devel',
        'embryo:runtime',
        'emotion:devel', 
        'enlightenment:devel',
        'ethumb:devel', 
        'evas:devel', 
        'gtk:runtime',
        'libexif:devel', 
        'pkgconfig:devel',
            ]
 
    def unpack(r):
        r.addArchive('http://download.enlightenment.org/releases/')

class LibchamplainGtk(AutoPackageRecipe):
    name = 'libchamplain-gtk'
    version = '0.2.9'
 
    buildRequires = ['atk:devel', 'cairo:devel', 'clutter-gtk:devel', 'clutter:devel', 'fontconfig:devel', 'freetype:devel', 'glib:devel', 'gtk-doc:runtime', 'gtk:devel', 'libchamplain:devel', 'pango:devel', 'pkgconfig:devel', 'Mesa:devel']
 
    def unpack(r):
        r.addArchive('http://libchamplain.pierlux.com/release/%(version)s/libchamplain-gtk-%(version)s.tar.gz')

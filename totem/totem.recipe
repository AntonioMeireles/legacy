loadRecipe('gnomepackage.recipe', label='conary.rpath.com@rpl:devel')
class Totem(GnomePackageRecipe):

    name = 'totem'
    version = '1.1.2'

    extraConfig = GnomePackageRecipe.extraConfig
    extraConfig += '--enable-gstreamer --disable-nvtv --disable-schemas-install --enable-mozilla --enable-nautilus'

    def unpack(r):
	r.addArchive('http://ftp.gnome.org/pub/GNOME/sources/%(name)s/1.1/%(name)s-%(version)s.tar.bz2')

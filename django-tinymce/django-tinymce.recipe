class DjangoTinyMCE(PackageRecipe):

    name = 'django-tinymce'
    version = '1.5'

    buildRequires = [
        'django:python',
        'python:devel',
        'python-setuptools:python',
    ]

    def setup(r):
        r.addArchive('http://django-tinymce.googlecode.com/files/')
        r.PythonSetup()

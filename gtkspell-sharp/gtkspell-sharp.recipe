class GtkspellSharp(AutoPackageRecipe):
    name = 'gtkspell-sharp'
    version = 'svn.r84987'
 
    buildRequires = ['gtk-sharp:cil', 'gtk-sharp:devellib', 'mono:cil', 'mono:devel', 'mono:runtime', 'pkgconfig:devel']
 
    def unpack(r):

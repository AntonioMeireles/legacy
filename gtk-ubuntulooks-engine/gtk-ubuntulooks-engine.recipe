class GtkUbuntuLooks(AutoPackageRecipe):

    buildRequires = [ 'atk:devel', 'cairo:devel', 'fontconfig:devel', 'freetype:devel', 'glib:devel', 'gtk:devel', 'libpng:devel', 'pango:devel', 'pkgconfig:devel', 'xorg-x11:devel', 'zlib:devel']

    name = 'gtk-ubuntulooks-engine'
    version = '0.9.11'

    def unpack(r):
        r.addArchive('http://archive.ubuntu.com/ubuntu/pool/main/u/ubuntulooks/ubuntulooks_%(version)s.orig.tar.gz')

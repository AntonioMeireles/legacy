loadSuperClass('binarypackage')
class rar(BinaryPackageRecipe):
    buildRequires=[ ]

    name = 'rar'
    version = '4.0.1'

    def unpack(r):
        r.addArchive('http://www.rarlab.com/rar/rarlinux-%(version)s.tar.gz')
	r.Install('rar', '%(bindir)s/')

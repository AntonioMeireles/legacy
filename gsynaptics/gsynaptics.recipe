class GSynaptics(PackageRecipe):
    buildRequires = [ 'perl-XML-Parser:perl','libgnomeui:devel' ]

    name = "gsynaptics"
    version = "0.9.2"


    def setup(r):
	r.addArchive('http://osdn.dl.sourceforge.jp/gsynaptics/15345/%(name)s-%(version)s.tar.gz')
	r.Configure()
	r.Make()
	r.MakeInstall()

loadSuperClass('gnomepackage')
class JsonGlib(GnomePackageRecipe,AutoPackageRecipe):
    name = 'json-glib'
    version = '0.10.4'
 
    def policy(r):
        r.Replace('libdir\=\$\{exec_prefix\}\/lib', 'libdir=${exec_prefix}/%(lib)s', '%(libdir)s/pkgconfig/json-glib-1.0.pc')

class Pytz(PackageRecipe):

    name = 'pytz'
    version = '2006p'

    buildRequires = ['python-setuptools:python',"python:devel"]

    def setup(r):
        r.addArchive('mirror://sourceforge/%(name)s/')
        r.PythonSetup()

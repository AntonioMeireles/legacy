class GaimGalago(PackageRecipe):
    buildRequires = [ 'libgalago:devel', 'dbus-glib:devellib',
        'dbus:devel', 'glib:devel', 'glibc:devel' ]

    name = "gaim-galago"
    version = "0.5.0"
    buildRequires = ['binutils:runtime', 'dbus-glib:devel', 'gawk:runtime', 'pkgconfig:devel', 'dbus:devel', 'glib:devel', 'glibc:devel', 'libgalago:devel']

    
    def setup(r):
        r.addArchive('http://www.galago-project.org/files/releases/source/%(name)s/%(name)s-%(version)s.tar.gz')
	r.Configure()
	r.Make()
	r.MakeInstall()

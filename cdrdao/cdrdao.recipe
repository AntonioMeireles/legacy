class cdrdao(PackageRecipe):
    buildRequires = [ "glibc:devel" ]
    name = "cdrdao"
    version = "1.1.9"

    def setup(r):
        r.addArchive('http://kent.dl.sourceforge.net/sourceforge/%(name)s/%(name)s-%(version)s.tar.gz')
        r.Configure()
        r.Make()
        r.MakePathsInstall()

class ndisgtk(PackageRecipe):
    buildRequires = [ 'desktop-file-utils:runtime', 'mono:runtime' ]
    name = "ndisgtk"
    version = "0.5"


    def setup(r):
    	r.addArchive('%(name)s-%(version)s.tar.gz')
        r.Make()
        r.MakeInstall()

	r.Requires('gnome-system-tools:runtime', '%(bindir)s/ndisgtk')

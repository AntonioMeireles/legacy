class GroupMono(GroupRecipe):
    name = 'group-mono'
    version = '1.1.9.2'
    #depCheck = True

    def setup(r):
        r.setLabelPath('foresight.rpath.org@fl:desktop', 'conary.rpath.com@rpl:rpl1', 'contrib.rpath.org@rpl:devel')
        r.addTrove("gecko-sharp","contrib.rpath.org@rpl:devel")
        r.addTrove("gecko-sharp","contrib.rpath.org@rpl:gecko-sharp1")
        r.addTrove("gsf-sharp")
        r.addTrove("gtk-sharp","contrib.rpath.org@rpl:devel")
        r.addTrove("gtk-sharp","contrib.rpath.org@rpl:gtk-sharp1")
        r.addTrove("gtksourceview-sharp")
        r.addTrove("mono")
        r.addTrove("group-mono-dev-tools", byDefault=False)

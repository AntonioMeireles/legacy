loadSuperClass('kdecorepackage')
class kdeToys(kdeCorePackageRecipe):

    name = 'kdetoys'
    version = '3.5.10'

    verify = False

    buildRequires = [
        'pcre:devel'
    ]

    modules = [
        'amor',
       ('keyes', 'eyesapplet'),
       ('kfifteen', 'fifteenapplet'),
        'kmoon',
        'kodo',
        'kteatime',
        'ktux',
        'kweather',
        'kworldwatch',
       ('kworldwatch', 'doc/kworldclock')
    ]


loadRecipe('gnomepackage.recipe')
class GnomeMag(GnomePackageRecipe):
    
    buildRequires = GnomePackageRecipe.buildRequires
    buildRequires.extend([ 'intltool:runtime', 'xorg-x11:devel',
                            'libbonobo:devel', 'gtk:devel', 'at-spi:devel',
                            'gettext:runtime' ])
    
    name = 'gnome-mag'
    version = '0.11.14'

    def setup(r):
	r.disableParallelMake()
	GnomePackageRecipe.setup(r)

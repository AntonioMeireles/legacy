class PythonDateUtil(PackageRecipe):
    name = 'python-dateutil'
    version = '1.5'

    buildRequires = [ 'python-setuptools:python', 'python:devel', ]

    def setup(r):
        r.addArchive('http://labix.org/download/%(name)s/%(name)s-%(version)s.tar.gz')
        r.PythonSetup()

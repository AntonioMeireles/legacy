class GstPython(AutoPackageRecipe):

    buildRequires = [ 'glib:devel', 'glibc:devel', 
			'gstreamer:devel', 'libxml2:devel', 'zlib:devel' ]

    name = 'gst-python'
    version = '0.10.3'
    def unpack(r):
	r.addArchive('http://gstreamer.freedesktop.org/src/%(name)s/%(name)s-%(version)s.tar.bz2')

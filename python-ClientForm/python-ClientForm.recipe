class PythonClientForm(PackageRecipe):
    name = 'python-ClientForm'
    version = '0.0.16'

    buildRequires = [ 'python:devel' ]

    def setup(r):
        r.addArchive('http://wwwsearch.sourceforge.net/ClientForm/src/ClientForm-%(version)s.tar.gz')
        r.PythonSetup()

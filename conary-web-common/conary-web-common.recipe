class ConaryWebCommon(BuildPackageRecipe):
    name = 'conary-web-common'
    version = '1.0'

    def setup(r):
        r.addArchive('ftp://download.rpath.com/conary/%(name)s-%(version)s.tar.bz2')
        r.Make()
        r.MakeInstall()

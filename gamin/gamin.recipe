class Gamin(PackageRecipe):
    buildRequires = [ 'glib:devel', 'pkgconfig:devel' ]

    name = 'gamin'
    version = '0.0.26'
    
    def setup(r):
	r.addArchive('http://www.gnome.org/~veillard/%(name)s/sources/%(name)s-%(version)s.tar.gz')

	r.Configure()
	r.Make()
	r.MakeInstall()

loadSuperClass('gnomepackage')
class Gamin(GnomePackageRecipe):

    name = 'gamin'
    version = '0.1.10'

    clearBuildRequires('gtk:devel')
    buildRequires = [
        'glib:devel', 'pkgconfig:devel', 'python:devel'
    ]

    extraConfig = '--disable-debug --with-python'


class PyhttlLIb2(AutoPackageRecipe):
    name = 'python-httplib2'
    version = '0.4.0'

    buildRequires = [ 'python:devel',
                      'python-setuptools:python'
                    ]

    def setup(r):
        r.addArchive('http://httplib2.googlecode.com/files/httplib2-%(version)s.tar.gz')
        r.PythonSetup()
        

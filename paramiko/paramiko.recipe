class Paramiko(PackageRecipe):
    name = 'paramiko'
    version = '1.7.6'

    buildRequires = []

    def setup(r):
        r.addArchive('http://www.lag.net/paramiko/download/')
        r.PythonSetup()
        r.PythonSetup('install')

class Paramiko(PackageRecipe):

    name = 'paramiko'
    version = '1.7.4'

    buildRequires = ['python-setuptools:python', 'python-ctypes:python', 'python:devel']

    def setup(r):
        r.addArchive('http://www.lag.net/paramiko/download/')
        r.PythonSetup()

class OpenobexApps(PackageRecipe):
    buildRequires = [ "autoconf", "openobex:devel", "bluez-libs:devel" ]

    name = "openobex-apps"
    version = "1.0.0"

    def setup(r):
        r.addArchive("http://easynews.dl.sourceforge.net/sourceforge/openobex/%(name)s-%(version)s.tar.gz")
	r.Configure()
	r.Make()
	r.MakeInstall()

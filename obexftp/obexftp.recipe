class Obexftp(PackageRecipe):
    buildRequires = [ "bluez-libs:devel", "bluez-utils", "openobex:devel" ]

    name = "obexftp"
    version = "0.10.7"

    def setup(r):
        r.addArchive("http://triq.net/obexftp/%(name)s-%(version)s.tar.gz")
	r.Configure()
	r.Make()
	r.MakeInstall()

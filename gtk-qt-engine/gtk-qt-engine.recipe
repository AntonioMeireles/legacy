loadSuperClass('kdepackage')
class GTKQTEngine(kdePackageRecipe):
    version = '1.1'
    name='gtk-qt-engine'

    buildRequires = [ 'gtk:devel', 'GConf:devel', 'ORBit2:devel',
        'esound:devel', 'expat:devel',  'gnome-vfs:devel',
        'kdesdk-scripts:runtime', 'libart_lgpl:devel', 'libbonobo:devel',
        'libbonoboui:devel', 'libgnome:devel', 'libgnomecanvas:devel',
        'libxml2:devel', 'popt:devel', 'cmake:runtime', 'gettext:runtime',
        'libXpm:devel', 'libXv:devel', 'openssl:devel', 'qt4-core:lib', 'qt4-gui:lib', 'qt4:devel' ]

    def unpack(r):
        r.addArchive('http://gtk-qt.ecs.soton.ac.uk/files/%(version)s/')

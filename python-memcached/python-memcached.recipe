class PythonMemcached(PackageRecipe):
    name = 'python-memcached'
    version = '1.45'

    buildRequires = [
        'python:devel', 'python-setuptools:python',
    ]

    def setup(r):
        r.addArchive('ftp://ftp.tummy.com/pub/%(name)s/')
        r.PythonSetup()

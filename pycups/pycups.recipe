class Pycups(CPackageRecipe):

    name = 'pycups'
    version = '1.9.45'

    buildRequires = [
        'cups:devel', 'make:runtime', 'python:devel'
    ]

    def setup(r):
        r.addArchive('http://cyberelk.net/tim/data/pycups/')
        r.Make()
        r.MakeInstall()


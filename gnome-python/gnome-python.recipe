loadSuperClass('gnomepackage')
class GnomePython(GnomePackageRecipe):
    name = 'gnome-python'
    version = '2.20.1'

    buildRequires = [ 'GConf:devel', 'glib:devel', 'gnome-vfs:devel',
        'libart_lgpl:devel', 'libbonobo:devel', 'libbonoboui:devel',
        'libgnomecanvas:devel', 'libgnome:devel', 'libgnomeui:devel',
        'libgtkhtml:devel', 'libxml2:devel', 'ORBit2:devel', 'pango:devel',
        'popt:devel', 'pygtk:devel', 'python:devel', 'cairo:devel',
        'fontconfig:devel', 'freetype:devel', 'libpng:devel', 'pygtk:python',
        'pygobject:python', ]

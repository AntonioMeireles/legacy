worker_processes         1;

events {
    use                  epoll;
    worker_connections   1024;
}

http {
    include              %(sysconfdir)s/nginx/mime.types;
    default_type         application/octet-stream;

    sendfile             on;
    keepalive_timeout    65;

    gzip                 on;
    gzip_min_length      1024;
    gzip_http_version    1.0;
    gzip_types           text/xml text/html text/plain text/css application/rss+xml application/atom+xml application/x-javascript;

    # include all conf files ending in .conf for vhost support, etc.
    include %(sysconfdir)s/nginx/conf.d/*.conf;
}


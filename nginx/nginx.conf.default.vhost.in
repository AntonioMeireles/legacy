# default server for static web pages
server {
    listen         80;
    server_name    localhost;
    root           %(servicedir)s/nginx/html;
    charset        utf-8;

    location / {
        index      index.html index.htm;
    }

    error_page     500 502 503 504  /50x.html;

    # include extra configuration for the default vhost
    include %(sysconfdir)s/nginx/conf.d/00-default*.custom;

}

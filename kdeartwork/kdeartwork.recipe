loadSuperClass('kdecorepackage.recipe')
class kdeArtwork(kdeCorePackageRecipe):

    name = 'kdeartwork'
    version = '3.5.10'

    verify = False

    buildRequires = [
        'kdebase-workspace:devel'
    ]

    modules = [
        ('kdeartwork-emoticons', 'emoticons'),
        ('kdeartwork-icewm-themes', 'icewm-themes'),
        ('kdeartwork-icon-themes', 'IconThemes'),
        ('kscreensaver-extra', 'kscreensaver'),
        ('kdeartwork-kwin-styles', 'kwin-styles'),
        ('kdeartwork-kworldclock', 'kworldclock'),
        ('kdeartwork-sounds', 'sounds'),
        ('kdeartwork-styles', 'styles'),
        ('kdeartwork-wallpapers', 'wallpapers')
    ]


class Cython(PackageRecipe):
    name = 'cython'
    version = '0.10.3'

    buildRequires = ['python:devel', 'python-setuptools:python']

    def setup(r):
        r.addArchive('http://cython.org/Cython-%(version)s.tar.gz')
        r.PythonSetup()

class GaimEncryption(AutoPackageRecipe):
    name = 'gaim-encryption'
    version = '3.0beta8'

    buildRequires = [ 'expat:devellib', 'gaim:devel', 'gtk:devel', 'pkgconfig:devel',
                      'nspr:devel', 'nss:devel', 'zlib:devellib' ]

    def unpack(r):
        r.addArchive('mirror://sourceforge/%(name)s/%(name)s-%(version)s.tar.gz')

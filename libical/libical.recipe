class LibICal(AutoPackageRecipe):

    name = 'libical'
    version = '0.42'

    buildRequires = [
        'bison:runtime', 'flex:runtime', 'perl:runtime'
    ]

    def unpack(r):

        r.addArchive('mirror://sourceforge/freeassociation/libical-%(version)s.tar.gz')


class cpufrequtils(AutoPackageRecipe):
    name = 'cpufrequtils'
    version = '0.4'
    buildRequires = ['sysfsutils:devel']

    def unpack(r):
        r.addArchive('http://www.kernel.org/pub/linux/utils/kernel/cpufreq/%(name)s-%(version)s.tar.gz')

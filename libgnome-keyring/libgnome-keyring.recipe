loadSuperClass('gnomepackage')

class LibGnomeKeyring(GnomePackageRecipe):
    name = 'libgnome-keyring'
    version = '2.29.4'

    buildRequires = [ 
        'eggdbus:devel',
        'libgcrypt:devel',
        'libgpg-error:devel',
        
  ]

    GnomePackageRecipe.extraConfig += ' --disable-static'
    patches = ['libgnome-keyring-no-match.patch']


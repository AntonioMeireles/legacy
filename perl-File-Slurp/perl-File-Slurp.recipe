loadSuperClass('cpanpackage.recipe')
class FileSlurp(CPANPackageRecipe):
    name='perl-File-Slurp'
    version='9999.13'

    buildRequires = [ ]

    def post(r):
        for i in r.buildRequires:
            r.Requires(i, r"\.pm")
        CPANPackageRecipe.post(r)


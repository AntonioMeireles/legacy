loadSuperClass('cpanpackage=conary.rpath.com@rpl:2')
class FileSlurp(CPANPackageRecipe):
    name='perl-File-Slurp'
    version='9999.13'

    buildRequires = [ 'perl:devel' ]

    def post(r):
        for i in r.buildRequires:
            r.Requires(i, r"\.pm")
        CPANPackageRecipe.post(r)



class GstFluendoMpegDemux(AutoPackageRecipe):

    buildRequires =  [ 'glib:devel', 'gstreamer:devel',
       'libxml2:devel', 'zlib:devel' ]
    
    name = 'gst-fluendo-mpegdemux'
    version = '0.10.3'

    externalArchive='http://gstreamer.freedesktop.org/src/%(name)s/%(name)s-%(version)s.tar.bz2'

    def unpack(r):
        r.addArchive('http://core.fluendo.com/gstreamer/src/%(name)s/%(name)s-%(version)s.tar.bz2')

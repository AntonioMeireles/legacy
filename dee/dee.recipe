class Dee(AutoPackageRecipe):
    name = 'dee'
    version = '0.5.18'
 
    buildRequires = [
            'dbus-glib:devel', 
            'glib:devel', 
            'gtk-doc:runtime', 
            'pkgconfig:devel', 
            'pygobject:python', 
            'vala:runtime'
                    ]
 
    def unpack(r):
        r.addArchive('http://launchpad.net/dee/trunk/%(version)s/+download/')

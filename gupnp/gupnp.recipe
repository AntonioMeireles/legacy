class Gupnp(AutoPackageRecipe):
    name = 'gupnp'
    version = '0.13.5'
 
    buildRequires = ['gssdp:devel', 'pkgconfig:devel', 'gtk-doc:runtime', 'python:devel', 'util-linux:devel']
 
    def unpack(r):
        r.addArchive('http://www.gupnp.org/sites/all/files/sources/')

class MenuCache(AutoPackageRecipe):
    name = 'menu-cache'
    version = '0.3.2'
 
    buildRequires = ['glib:devel', 'pkgconfig:devel']
 
    def unpack(r):
        r.addArchive('http://downloads.sourceforge.net/lxde/%(name)s-%(version)s.tar.gz')

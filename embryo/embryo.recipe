class embryo(AutoPackageRecipe):
    name = 'embryo'
    version = "1.0.0"

    buildRequires = ['pkgconfig:devel', 'automake:runtime', 'autoconf:runtime', 'libtool:runtime']
    def unpack(r):
        r.addArchive("http://download.enlightenment.org/releases/embryo-%(version)s.tar.gz")

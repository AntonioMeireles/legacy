loadRecipe('gnomepackage.recipe')
class TelepathyMissionControl(GnomePackageRecipe):

    name = 'telepathy-mission-control'
    version = '4.49'

    buildRequires = ['GConf:devel', 'ORBit2:devel','dbus-glib:runtime', 'libtelepathy:devel']
    externalArchive ='mirror://sourceforge/mission-control/'
    def unpack(r):
        GnomePackageRecipe.unpack(r)
        # http://lists.freedesktop.org/archives/telepathy/2007-November/001249.html
        r.addPatch('fix-contact-list.patch')



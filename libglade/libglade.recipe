loadRecipe('gnomepackage.recipe')
class Libglade(GnomePackageRecipe):

    buildRequires = GnomePackageRecipe.buildRequires
    buildRequires.extend([ 'libxml2:lib', 'gtk:devel', 'fontconfig:runtime',
		           'pango:devel', 'libtool:runtime' ]) 
   
    name = 'libglade'
    version = '2.4.1'

    docs = GnomePackageRecipe.docs
    docs.append('test-libglade.c')

loadSuperClass('gnomepackage')
class Libglade(GnomePackageRecipe):
    name = 'libglade'
    version = '2.6.2'

    buildRequires = [ 'libxml2:devel', 'gtk:devel', 'fontconfig:runtime',
                      'pango:devel', 'libtool:runtime', 'atk:devel',
                      'glib:devel', 'cairo:devel', 'fontconfig:devel',
                      'freetype:devel', 'libpng:devel' ]


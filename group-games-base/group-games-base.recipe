class Dist(GroupRecipe):
    name = 'group-games-base'
    version = '0.9.0.3'
    #depCheck = True

    def setup(r):
        r.setLabelPath('foresight.rpath.org@fl:desktop', 'conary.rpath.com@rpl:devel', 'contrib.rpath.com@rpl:devel')
        r.addTrove("gnome-games")
        r.addTrove("gnome-games-extra-data")
        r.addTrove("freeciv")
        #Remove gnome-sudoku due to compile issues and include GNUDoku as a temporary replacement
        #r.addTrove("gnome-sudoku")
        r.addTrove("GNUDoku")
        r.addTrove("jotto")

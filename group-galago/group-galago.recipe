class Dist(GroupRecipe):
    name = 'group-galago'
    version = '0.3.3'
    #depCheck = True

    def setup(r):
        r.setLabelPath('foresight.rpath.org@fl:desktop')

        r.addTrove("libgalago")
        r.addTrove("galago-daemon")
        r.addTrove("eds-feed")
        r.addTrove("gaim-galago")
        r.addTrove("libgalago-gtk")
        r.addTrove("gnome-presence-applet")
        r.addTrove("libnotify")
        r.addTrove("notification-daemon")

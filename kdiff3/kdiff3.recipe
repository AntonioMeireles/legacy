loadSuperClass('kdepackage')
class Kdiff3(kdePackageRecipe):
    name = 'kdiff3'
    version = '0.9.92'
 
    buildRequires = ['gettext:runtime', 'libkonq:devel']
 
    def unpack(r):
        r.addArchive('mirror://sourceforge/kdiff3/kdiff3-%(version)s.tar.gz')

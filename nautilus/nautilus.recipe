loadRecipe('gnomepackage.recipe', label='conary.specifix.com@spx:devel')
class Nautilus(GnomePackageRecipe):
    
    buildRequires = [ 'glib:devel', 'gtk:devel', 'pango:devel',
                      'libgnomeui:devel', 'eel:devel',
                      'gail:devel', 'gnome-vfs:devel',
                      'gnome-desktop:devel', 'gamin:devel',
                      'desktop-file-utils:runtime',
                      'startup-notification:devel',
                      'libexif:devel', 'librsvg:devel',
                      'fontconfig:runtime', 'xorg-x11:devel',
                      'libxml2:devel', 'intltool:runtime' ]
               
    name = 'nautilus'

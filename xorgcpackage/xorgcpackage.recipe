loadSuperClass('xorgdatapackage.recipe')
class XorgCPackageRecipe(XorgDataPackageRecipe, CPackageRecipe):
    name = 'xorgcpackage'
    version = '1'

    buildRequires = [ 'pkgconfig:devel' ]

    def setup(r):
        if r.__class__.__name__ == 'XorgCPackageRecipe':
            return

        XorgDataPackageRecipe.setup(r)

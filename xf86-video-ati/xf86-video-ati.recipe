class Xf86VideoAti(XorgDriverPackageRecipe):
    name = 'xf86-video-ati'
    version = '6.6.3'
    component = 'driver'

    buildRequires = [ 'glproto:devel', 'libdrm:devel', 'Mesa:devel',
        'xf86driproto:devel', 'xf86miscproto:devel', 'xineramaproto:devel',
        'ghostscript:runtime' ]


loadSuperClass('binarypackage=conary.rpath.com@rpl:devel')
class IScan(BinaryPackageRecipe):

    buildRequires = [ ]
    name = 'iscan'
    version = '2.3.0'

    def unpack(r):

        # From http://www.avasys.jp/english/linux_e/dl_scan.html
        rpms = [ 'iscan-2.3.0-1.c2.i386.rpm', 
                'iscan-plugin-gt-f500-1.0.0-1.c2.i386.rpm' ]

        for rpm in rpms:
            r.addArchive(rpm, dir='/')

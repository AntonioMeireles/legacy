loadRecipe('gnomepackage.recipe')
class Libgnomeprintui(GnomePackageRecipe):
    
    buildRequires = GnomePackageRecipe.buildRequires
    buildRequires.extend([ 'gtk:devel', 'libgnomeprint:devel',
                           'libgnomecanvas:devel', 'freetype:devel',
                           'gnome-icon-theme:devel'])

    name = 'libgnomeprintui'
    version = '2.8.0'

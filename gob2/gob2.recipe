class Gob2(PackageRecipe):
    buildRequires = [ 'flex:runtime', "glibc:devel", "pkgconfig:devel", "glib:devel","bison:runtime", "coreutils:runtime" ]

    name = "gob2"
    version = "2.0.14"

    def setup(r):
        r.addArchive("http://ftp.5z.com/pub/gob/%(name)s-%(version)s.tar.gz")
	r.Configure()
	r.Make()
        r.MakeInstall()


class Gsl(AutoPackageRecipe):

    name = 'gsl'
    version = '1.12'

    buildRequires = [
        'install-info:runtime'
    ]

    def unpack(r):

        r.addArchive('ftp://ftp.gnu.org/gnu/gsl/')


class Xfce4QuickLauncherPlugin(AutoPackageRecipe):
    name = 'xfce4-quicklauncher-plugin'
    version = '1.9.4'

    buildRequires = ['gettext:runtime', 'intltool:runtime', 'pkgconfig:devel', 'atk:devel', 'cairo:devel', 'expat:devel', 'fontconfig:devel', 'freetype:devel', 'glib:devel', 'gtk:devel', 'libX11:devel', 'libXrender:devel', 'libpng:devel', 'libxcb:devel', 'libxfce4util:devel', 'libxfcegui4:devel', 'pango:devel', 'pixman:devel', 'xcb-util:devel', 'xfce4-panel:devel', 'zlib:devel']

    def unpack(r):
        r.addArchive('http://goodies.xfce.org/releases//%(name)s/')

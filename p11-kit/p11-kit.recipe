class P11Kit(AutoPackageRecipe):
    name = 'p11-kit'
    version = '0.12'
 
    buildRequires = ['gettext:runtime', 'gtk-doc:runtime', 'intltool:runtime']
 
    def unpack(r):
        r.addArchive('http://p11-glue.freedesktop.org/releases/')

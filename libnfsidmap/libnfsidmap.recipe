class LibNfsIdmap(AutoPackageRecipe):
    name = 'libnfsidmap'
    version = '0.20'

    buildRequires = [ 'openldap:devel', ]

    def unpack(r):
        r.addArchive('http://www.citi.umich.edu/projects/nfsv4/linux/%(name)s/')

class Grip(AutoPackageRecipe):
    name = 'grip'
    version = '3.3.1'

    buildRequires = ['cdparanoia:devel', 'autoconf:runtime', 'vte:devel']

    def unpack(r):
        r.addArchive('http://easynews.dl.sourceforge.net/sourceforge/%(name)s/%(name)s-%(version)s.tar.gz')


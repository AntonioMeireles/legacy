class AbiwordExtras(AutoPackageRecipe):
    name = 'abiword-extras'
    version = '2.5.2'
 
    buildRequires = ['abiword:devel', 'pkgconfig:devel']
 
    def unpack(r):
        r.addArchive('http://www.abisource.com/downloads/abiword/%(version)s/source/')

class UUID(PackageRecipe):

    name = 'python-uuid'
    version = '1.30'

    buildRequires = [
        'python:devel',
        'python-setuptools:python',
    ]

    def setup(r):
        r.macros.name = 'uuid'
        r.addArchive('http://pypi.python.org/packages/source/u/uuid/')
        r.PythonSetup()

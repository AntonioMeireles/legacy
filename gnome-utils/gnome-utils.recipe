loadRecipe('gnomepackage.recipe', label='conary.specifix.com@spx:devel')
class GnomeUtils(GnomePackageRecipe):
    name = 'gnome-utils'
    version = '2.10.1'
    buildRequires = [ 'gnome-vfs:devel', 'libgnome:devel', 'libgnomeui:devel',
		      'gnome-desktop:devel', 'gnome-panel:devel' ]

    def unpack(r):
	GnomePackageRecipe.unpack(r)
        r.Replace('Exec=','Exec=gksudo ','logview/gnome-system-log.desktop.in')


class Suitypy(BuildPackageRecipe):
    name = 'SuityPy'
    version = '0.1.1'

    buildRequires = [ 'ZSI:runtime', 'python:devel', 'wget:runtime' ]

    def setup(r):
        r.addArchive('http://people.rpath.com/~agrimm/SuityPy/')
        r.Make('libdir=%(libdir)s')
        r.MakeInstall('libdir=%(libdir)s')

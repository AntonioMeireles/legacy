class LIBSynce(CPackageRecipe):
    name = 'libsynce'
    version = '0.15.1'
 
    buildRequires = ['dbus-glib:devel', 'dbus:devel', 'glib:devel',
        'pkgconfig:devel', 'hal:devel']
 
    def setup(r):
        r.addArchive('mirror://sourceforge/synce/SynCE/%(version)s/')
        r.Configure()
        r.Make()
        r.MakeInstall()

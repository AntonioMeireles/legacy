class Eeze(AutoPackageRecipe):
    name = 'eeze'
    version = '1.0.1'
 
    buildRequires = ['ecore:devel', 'eina:devel', 'pkgconfig:devel', 'udev:devel']
 
    def unpack(r):
        r.addArchive('http://download.enlightenment.org/releases/eeze-%(version)s.tar.gz')

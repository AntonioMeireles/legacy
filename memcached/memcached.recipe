class MemCached(AutoPackageRecipe):
    name = 'memcached'
    version = '1.4.0'

    buildRequires = [ 'libevent:devel', ]

    def unpack(r):
        r.addArchive('http://memcached.googlecode.com/files/')
        r.Replace('-Werror ', '', 'configure')

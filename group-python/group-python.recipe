class Dist(GroupRecipe):
    name = 'group-python'
    version = '2.4.1'
    #depCheck = True

    def setup(r):
        r.setLabelPath('foresight.rpath.org@fl:desktop', 'conary.rpath.com@rpl:1', 'contrib.rpath.org@rpl:devel')
        r.addTrove("kid")
        r.addTrove("PIL")
        r.addTrove("pycrypto")
        r.addTrove("python")
        #r.addTrove("python-imaging")
        r.addTrove("python-numeric")
        r.addTrove("pygtk")
        r.addTrove("pyorbit")
        r.addTrove("Pyrex")
	r.addTrove("PyXML")

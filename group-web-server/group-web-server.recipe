class WebServer(GroupRecipe):
    name = 'group-web-server'
    version = '0.9.1'
    #depCheck = True
    #autoResolve = True

    def setup(r):
        r.setLabelPath('foresight.rpath.org@fl:desktop', 'conary.rpath.com@rpl:rpl1', 'contrib.rpath.org@rpl:devel')
        r.addTrove("httpd")
        r.addTrove("mod_ssl", source='httpd')
	r.addTrove("mod_python")
        r.addTrove("php")
        r.addTrove("php-pgsql", source='php')
        r.addTrove("php-mysql", source='php')


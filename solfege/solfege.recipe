class solfege(AutoPackageRecipe):
    name = "solfege"
    version = "3.0.0"
    buildRequires = [ 'gtk:lib', 'pygtk:runtime', 'python:runtime','gettext:runtime', 'pkgconfig:devel', 'swig:runtime', 'texinfo:runtime', 'desktop-file-utils:runtime' ]
    def unpack(r):
        r.addArchive('http://puzzle.dl.sourceforge.net/sourceforge/%(name)s/%(name)s-%(version)s.tar.gz')

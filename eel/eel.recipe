    buildRequires = ['pango:devel', 'gtk:devel', 'libgnomeui:devel',
			'gnome-vfs:devel', 'librsvg:devel', 'libbonobo:devel',
			'libxml2:devel', 'libart_lgpl:devel', 'gail:devel',
			'GConf:devel', 'ORBit2:devel', 'atk:devel', 'cairo:devel', 
			'fontconfig:devel', 'freetype:devel', 'glib:devel', 
			'glibc:devel', 'gnome-desktop:devel', 'gnome-menus:devel', 
			'libbonoboui:devel', 'libglade:devel', 'libgnome:devel', 
			'libgnomecanvas:devel', 'libpng:devel', 'popt:devel', 
			'startup-notification:devel', 'gnome-keyring:devel' ]


    name = 'eel'
    version = '2.14.1'
    
    docs = ['AUTHORS', 'COPYING', 'ChangeLog', 'NEWS', 'README', 'COPYING.LIB']

    def unpack(r):
	GnomePackageRecipe.unpack(r)
        r.macros.optflags = '-g -Os'


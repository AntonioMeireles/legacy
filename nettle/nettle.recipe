class Nettle(AutoPackageRecipe):
    name = 'nettle'
    version = '2.0'

    buildRequires = ['libstdc++:devel', 'gmp:devel', 'openssl:devel', 'glibc:devel', 'm4:runtime']

    def unpack(r):
        r.addArchive('http://www.lysator.liu.se/~nisse/archive/')

class Ircp(PackageRecipe):
    buildRequires = [ "openobex:devel" ]

    name = "ircp"
    version = "0.3"

    def setup(r):
        r.addArchive("http://easynews.dl.sourceforge.net/sourceforge/openobex/%(name)s-%(version)s.tar.gz")
	r.Configure()
	r.Make()
	r.MakeInstall()

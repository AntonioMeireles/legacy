class GupnpVala(AutoPackageRecipe):
    name = 'gupnp-vala'
    version = '0.6'
 
    buildRequires = ['vala:devellib', 'pkgconfig:devel', 'vala:runtime', 'gupnp-ui:devellib', 'gupnp-av:devellib']
 
    def unpack(r):
        r.addArchive('http://www.gupnp.org/sources/bindings/')

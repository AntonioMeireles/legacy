loadSuperClass('kdecorepackage')
class kdewebdev(kdeCorePackageRecipe):

    name = 'kdewebdev'
    version = '3.5.10'

    verify = False

    buildRequires = [
        'cervisia:devel', 'libxml2:devel', 'libxslt:devel', 'pcre:devel',
        'perl:runtime'
    ]

    modules = [
        'kfilereplace',
        'kimagemapeditor',
        'klinkstatus',
        'kommander',
        'kxsldbg',
       ('kxsldbg', 'doc/xsldbg'),
        'quanta'
    ]


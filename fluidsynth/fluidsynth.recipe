class Fluidsynth(AutoPackageRecipe):
    name = 'fluidsynth'
    version = '1.0.9'
 
    buildRequires = ['alsa-lib:devel', 'jack:devel', 'ncurses:devel', 'pkgconfig:devel', 'portaudio:devel', 'pulseaudio:devel', 'readline:devel']
 
    def unpack(r):
        r.addArchive('http://www.very-clever.com/download/nongnu/fluid/')

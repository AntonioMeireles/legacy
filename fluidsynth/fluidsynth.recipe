class Fluidsynth(AutoPackageRecipe):
    name = 'fluidsynth'
    version = '1.1.1'
 
    buildRequires = ['alsa-lib:devel', 'jack:devel', 'libsndfile:devel', 'ncurses:devel', 'pkgconfig:devel', 'portaudio:devel', 'pulseaudio:devel', 'readline:devel']
 
    def unpack(r):
        r.addArchive('http://mirrors.zerg.biz/nongnu/fluid/')

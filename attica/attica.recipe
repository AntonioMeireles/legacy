loadSuperClass('cmakepackage')
class Attica(CMakePackageRecipe):

    name = 'attica'
    version = '0.2.0'

    buildRequires = [
        'libstdc++:devel', 'qt4:devel'
    ]

    def unpack(r):

        r.addArchive('mirror://kde/stable/attica/')


class VolumeIcon(AutoPackageRecipe):
    name = 'volumeicon'
    version = '0.2.1'
 
    buildRequires = ['alsa-lib:devel', 'atk:devel', 'cairo:devel', 'pkgconfig:devel', 'fontconfig:devel', 'freetype:devel', 'glib:devel', 'gtk:devel', 'pango:devel']
 
    def unpack(r):
        r.addArchive('http://softwarebakery.com/maato/files/%(name)s/'
			'%(name)s-%(version)s.tar.gz')

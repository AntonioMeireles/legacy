loadRecipe('gnomepackage.recipe')
class TelepathySalut(GnomePackageRecipe):

    name = 'telepathy-salut'
    version = '0.1.3'

    buildRequires = ['avahi-glib:devel', 'avahi:devel', 'dbus-glib:runtime',
                     'krb5:devel', 'openssl:devel', 'telepathy-glib:devel']
    externalArchive ='http://telepathy.freedesktop.org/releases/%(name)s/%(name)s_%(version)s.orig.tar.gz'



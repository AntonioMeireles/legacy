class Dmidecode(CPackageRecipe):
    name = 'dmidecode'
    version = '2.9'

    def setup(r):
        r.addArchive('http://savannah.nongnu.org/download/%(name)s/')
        r.Make()
        r.MakeInstall('prefix=/usr')

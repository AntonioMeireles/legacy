class GtkThemeForesightStudio(PackageRecipe):
    name = 'gtk-theme-foresight-studio'
    version = '0.2'

    buildRequires = [ 'gtk:devel' ]

    def setup(r):
        r.addArchive('%(name)s-%(version)s.tar.bz2', dir='%(datadir)s/themes/')

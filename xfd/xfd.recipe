    component = 'app'

    buildRequires = [ 'fontconfig:devel', 'freetype:devel', 'libICE:devel',
        'libSM:devel', 'libXaw:devel', 'libXext:devel', 'libXft:devel',
        'libXmu:devel', 'libXpm:devel', 'libXrender:devel', 'libXt:devel',
        'zlib:devel' ]

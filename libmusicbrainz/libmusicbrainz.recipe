class LibMusicBrainz(PackageRecipe):
    name = 'libmusicbrainz'
    version = '2.1.1'

    buildRequires = [ ]

    def setup(r):
        r.addArchive('ftp://ftp.musicbrainz.org/pub/musicbrainz/%(name)s-%(version)s.tar.gz')

        r.Configure()
        r.Make()
        r.MakeInstall()

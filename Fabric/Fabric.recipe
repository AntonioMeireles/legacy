class Fabric(PackageRecipe):
    name = 'Fabric'
    version = '0.9.3'

    buildRequires = [
        'paramiko:python',
        'pycrypto:python',
        'python-setuptools:python',
        'python:devel',
    ]

    def setup(r):
        r.addArchive("http://pypi.python.org/packages/source/F/%(name)s/")
        r.PythonSetup()

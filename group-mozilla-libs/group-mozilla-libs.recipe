class Dist(GroupRecipe):
    name = 'group-mozilla-libs'
    version = '1.7.8'
    #depCheck = True

    def setup(r):
        r.setLabelPath('conary.foresightlinux.com@fl:desktop', 'conary.rpath.com@rpl:devel', 'contrib.rpath.com@rpl:devel')
        
        r.addTrove("mozilla-mail:lib")
        r.addTrove("mozilla:data")
        r.addTrove("mozilla:lib")
        r.addTrove("mozilla-nspr:lib")
        r.addTrove("mozilla-nss:lib")

class Dist(GroupRecipe):
    name = 'group-mozilla-libs'
    version = '1.7.12'
    #depCheck = True

    def setup(r):
        r.setLabelPath('foresight.rpath.org@fl:desktop', 'conary.rpath.com@rpl:1', 'contrib.rpath.org@rpl:devel')

        r.addTrove("mozilla-mail:lib", "conary.rpath.com@rpl:1")
        r.addTrove("mozilla:data", "conary.rpath.com@rpl:1")
        r.addTrove("mozilla:lib", "conary.rpath.com@rpl:1")
        r.addTrove("mozilla-nspr", "conary.rpath.com@rpl:1")
        r.addTrove("mozilla-nss", "conary.rpath.com@rpl:1")

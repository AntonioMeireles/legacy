class gpgme(AutoPackageRecipe) :
    name = 'gpgme'
    version = '1.1.4'
    buildRequires = [ 'libgpg-error:devel', 'gnupg:runtime',
        'install-info:runtime', 'pkgconfig:devel', 'pinentry:runtime', ]

    def unpack(r):
        r.addArchive('ftp://ftp.gnupg.org/gcrypt/gpgme/')

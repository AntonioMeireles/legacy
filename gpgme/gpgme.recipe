class gpgme(AutoPackageRecipe) :
    name = 'gpgme'
    version = '1.1.2'
    buildRequires = [ 'libgpg-error:devel', 'gnupg:runtime',
        'install-info:runtime', 'pkgconfig:devel', ]

    def unpack(r):
        r.addArchive('ftp://ftp.gnupg.org/gcrypt/gpgme/gpgme-%(version)s.tar.gz')

class FastOpen(PackageRecipe):
    
    buildRequires = [ 'gtk-sharp:devellib', 'mono:devel' ]
                          
    name = 'fastopen'
    version = '0.2.1'

    def setup(r):
	r.addArchive('http://mspace.berlios.de/FastOpen/Releases/FastOpen-%(version)s.tar.gz')
	r.mainDir('FastOpen-%(version)s')
	r.Configure()
	r.Make()
	r.MakeInstall()

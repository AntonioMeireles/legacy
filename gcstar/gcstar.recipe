class Gcstar(AutoPackageRecipe):
    name = 'gcstar'
    version = '1.5.0'
 
    buildRequires = []
 
    def setup(r):
        r.addArchive('http://download.gna.org/gcstar/%(name)s-%(version)s.beta1.tar.gz')
	r.Run('./install --prefix=%(destdir)s%(prefix)s/')

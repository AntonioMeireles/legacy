class LibGPGError(AutoPackageRecipe):
    name = "libgpg-error"
    version = "1.0"

    def unpack(r):
        r.addArchive('ftp://ftp.gnupg.org/gcrypt/%(name)s/%(name)s-%(version)s.tar.gz', keyid='57548DCD')


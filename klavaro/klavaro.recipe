loadSuperClass('gnomepackage')
class Klavaro(GnomePackageRecipe):

    name = 'klavaro'
    version = '1.2.1'

    buildRequires = ['curl:devel', 'glitz:devel', 'gtkdatabox:devel',
'libsexy:devel', 'openssl:devel', 'xcb-util:devel']

    externalArchive = "mirror://sourceforge/%(name)s/"

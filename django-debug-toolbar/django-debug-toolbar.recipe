class DjangoDebugToolbar(PackageRecipe):
    name = 'django-debug-toolbar'
    version = '0.8.4'

    buildRequires = [ 'python-setuptools:python', 'python:devel']

    def setup(r):
        r.addGitSnapshot('git://github.com/robhudson/django-debug-toolbar.git',
            tag=r.version)
        r.PythonSetup()


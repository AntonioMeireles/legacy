loadSuperClass('binarypackage')
class AppinventorSetup(BinaryPackageRecipe):
    name = 'appinventor-setup'
    version = '1.1'
 
    buildRequires = ['binutils:runtime']
 
    def unpack(r):
        r.addArchive('http://dl.google.com/dl/appinventor/installers/linux/appinventor-setup_1.1_all.deb', dir='/')

